#include<avr/io.h>
#include<util/delay.h>
int main()
{
DDRC = 0XFF;
while(1)
{
PORTC = 0X05;
_delay_ms(2000);
PORTC = 0X0A;
_delay_ms(2000);
PORTC = 0X04;
_delay_ms(2000);
PORTC = 0X01;
_delay_ms(2000);
PORTC = 0X08;
_delay_ms(2000);
PORTC = 0X02;
_delay_ms(2000);
PORTC = 0X06;
_delay_ms(2000);
PORTC = 0X09;
_delay_ms(2000);
PORTC = 0X00;
_delay_ms(2000);
}
}

